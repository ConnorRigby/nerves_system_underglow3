		function = LED_FUNCTION_STATUS;
	};
	multi-led@4 {
		reg = <0x4>;
		color = <LED_COLOR_ID_RGB>;
		function = LED_FUNCTION_STATUS;
	};
	multi-led@5 {
		reg = <0x5>;
		color = <LED_COLOR_ID_RGB>;
		function = LED_FUNCTION_STATUS;
	};
	multi-led@6 {
		reg = <0x6>;
		color = <LED_COLOR_ID_RGB>;
		function = LED_FUNCTION_STATUS;
	};
	multi-led@7 {
		reg = <0x7>;
		color = <LED_COLOR_ID_RGB>;
		function = LED_FUNCTION_STATUS;
	};
	multi-led@8 {
		reg = <0x8>;
		color = <LED_COLOR_ID_RGB>;
		function = LED_FUNCTION_STATUS;
	};
	multi-led@9 {
		reg = <0x9>;
		color = <LED_COLOR_ID_RGB>;
		function = LED_FUNCTION_STATUS;
